<div class="row">
	<div class="container">
		@if (User.IsInRole(AppointmentScheduling.Helper.Helper.admin))
		{
			<div class="form-group row col-2" id="divDoctorList">
				<label>Select Doctor</label>
				@Html.DropDownList("doctorId", new SelectList(ViewBag.DoctorList, "Id", "Name"), new { @class = "form-control select2" })

			</div>

		}
		<div id="calendar"></div>
	</div>
</div>

@section Scripts{

	<script src="~/js/script-custom-calendar.js"></script>

}